<h1> Sign up below:</h1>
 <%= form_for @user do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %><br><br>

   <%= f.label :password %>
   <%= f.password_field :password %><br><br>

   <%= f.label :'confirm password' %>
   <%= f.password_field :password_confirmation %><br>

   <br>
   <%= f.submit %>
<% end %>
